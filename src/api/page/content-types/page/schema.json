{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.home-hero",
        "blocks.page-hero",
        "blocks.ticket-block",
        "blocks.faq",
        "blocks.info-blocks",
        "blocks.countdown",
        "blocks.grid",
        "blocks.archive",
        "blocks.spacer",
        "blocks.news"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ctas": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "meta.seo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "primary_color": {
      "type": "string"
    },
    "marquee_text_top": {
      "type": "string"
    },
    "marquee_text_bottom": {
      "type": "string"
    }
  }
}
