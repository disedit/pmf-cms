{
  "kind": "singleType",
  "collectionName": "settings",
  "info": {
    "singularName": "setting",
    "pluralName": "settings",
    "displayName": "Settings",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "site_name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "Potries Music Fest"
    },
    "menu_items": {
      "displayName": "Menu Item",
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "marquee_text": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "marquee_speed": {
      "type": "decimal",
      "default": 4,
      "required": true
    },
    "footer_logos": {
      "displayName": "Footer Logos",
      "type": "component",
      "repeatable": true,
      "component": "footer.footer-logos",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "footer_text": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "socials": {
      "displayName": "socials",
      "type": "component",
      "repeatable": true,
      "component": "global.socials"
    },
    "footer_links": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "footer_legal": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "meta.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
